<section>

  <div class="container">
    <div class="row mt-4">

      <div class="text-right mb-3">
        <a href=" /admin/add-product" class="btn btn-primary">Add Product</a>
      </div>


    </div>
    <table class="table mt-5">
      <thead>
        <tr>
          <th scope="col">NO</th>
          <th scope="col">Name</th>
          <th scope="col">Category</th>
          <th scope="col">Description</th>
          <th image="col">Image</th>
        </tr>
      </thead>
      <tbody>
        {{#each Products}}
        <tr>
          <th scope="row">1</th>
          <td>{{this.name}}</td>
          <td>{{this.category}}</td>
          <td>{{this.descripton}}</td>
          <td>
            <image style="width:50px;height:50px" src="{{this.image}}">
          </td>
        </tr>
        {{/each}}
      </tbody>
    </table>


  </div>
</section>